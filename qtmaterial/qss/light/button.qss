/* VectorButton_I20 */
#VectorButton_I20 {
    border: none;
    background-color: transparent;
    color:#9aa0aa; 
    min-width: 20px;
    max-width: 20px;
    width: 20px;
    min-height: 20px;
    max-height: 20px;
    height: 20px;  
    font-size: 20px;
}

#VectorButton_I20:hover {
    color:#5b6474; 
}

#VectorButton_I20:pressed {
    color:#5b6474; 
}

#VectorButton_I20:disabled {
    color:#f0f3f6; 
}

#VectorButton_I20:checked {
    color:#5b6474; 
}

